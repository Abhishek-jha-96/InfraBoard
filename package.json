{
  "name": "infra-board",
  "version": "1.0.0",
  "author": "Arya Soni",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && npm run lint:scss",
    "lint:scss": "stylelint 'app/**/*.scss' --fix || true",
    "lint:js": "next lint",
    "test:unit": "jest --passWithNoTests",
    "test:unit:update": "jest --passWithNoTests --updateSnapshot",
    "type:check": "tsc",
    "cypress": "cypress",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.3",
    "@types/react": "^16.14.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "axios": "^1.6.5",
    "babel-jest": "^29.7.0",
    "cypress": "^13.6.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^12.0.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.1.2",
    "stylelint": "^16.2.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "*.{js,jsx,ts,tsx}": "eslint",
    "*.{css,scss}": "stylelint"
  },
  "description": "Feel free to use this Boilerplate as a starting point for your own Next.js Projects.",
  "main": "next.config.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xerocodee/InfraBoard.git"
  },
  "keywords": [
    "infraboard",
    "terraform"
  ],
  "bugs": {
    "url": "https://github.com/xerocodee/InfraBoard/issues"
  },
  "homepage": "https://github.com/xerocodee/InfraBoard#readme"
}
